{
  "name": "formulistone-website",
  "version": "0.1.0",
  "scripts": {
    "format": "prettier --write . && npm run prisma:format && npm run next:lint",
    "clean": "rm -rf .next",
    "next:lint": "next lint --fix",
    "next:start": "next start",
    "next:build": "npm run clean && npm run prisma:generate && next build",
    "prisma:init": "prisma init",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "prisma:reset": "npx prisma db push --force-reset",
    "prisma:update": "npm run prisma:generate && npm run prisma:push",
    "dev": "next dev",
    "prod": "npm run prisma:generate && npm run next:build"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.7.1",
    "@vercel/analytics": "^1.1.2",
    "@vercel/speed-insights": "^1.0.10",
    "next": "14.1.1",
    "next-auth": "^4.24.5",
    "next-safe-action": "^6.0.1",
    "nodemailer": "^6.9.11",
    "react": "^18",
    "react-dom": "^18",
    "react-image-file-resizer": "^0.4.8",
    "react-photo-album": "^2.3.1",
    "yet-another-react-lightbox": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "autoprefixer": "^10.0.1",
    "clsx": "^2.1.0",
    "daisyui": "^4.4.24",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.13.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "prisma": "^5.7.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
